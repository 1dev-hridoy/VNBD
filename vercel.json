{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/app.js",
            "dest": "/app.js"
        },
        {
            "src": "/hero.png",
            "dest": "/hero.png"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}